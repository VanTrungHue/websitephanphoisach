@model IEnumerable<WebsitePhanPhoiSach.Models.phieuxuat>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Phiếu xuất</h2>

<p>
    @Html.ActionLink("Thêm mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Người nhận ")
        </th>
        <th>
            @Html.DisplayName("Ngày xuất")
        </th>
        <th>
            @Html.DisplayName("Tên đại lý")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nguoinhan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ngayxuat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.daily.tendl)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.idpx }) |
            @Html.ActionLink("Chi tiết phiếu xuất", "Details", new { id=item.idpx }) 
          
        </td>
    </tr>
}

</table>
